{
    "name": "gufy/pdftohtml-php",
    "description": "PDF to HTML converter with PHP using Poppler-utils",
    "license": "MIT",
    "authors": [
        {
            "name": "Mochamad Gufron",
            "email": "mgufronefendi@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "spdload/php-html-parser",
                "version": "1.7.1",
                "type": "package",
                "source": {
                    "url": "git@github.com:spdload/php-html-parser.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "spdload/php-html-parser": "^1.7",
        "illuminate/config": "~5"
    },
    "require-dev":{
        "phpunit/phpunit":"~4",
        "satooshi/php-coveralls": "^1.0"
    },

    "autoload": {
        "psr-0": {
            "Gufy": "src"
        },
        "psr-4": {
            "Gufy\\PdfToHtml\\": "src/"
        }
    }
}
